<?xml version="1.0" encoding="UTF-8"?>
<!--DOCTYPE workflow PUBLIC "-//W3C//DTD WORKFLOW 1.0//EN" "http://www.pgist.org/wfengine/workflow.dtd"-->
<workflow name="LIT Flow 0001">


  <environment>
    <var name="totalCount" value="1"/>
  </environment>
  
  
  <process name="process1">
    <environment>
      <var name="totalCount" value="1"/>
    </environment>
    <sequence>
      <task name="Input your name" class="org.pgist.wfengine.InputNamePerformer"/>
      <task name="Greeting" class="org.pgist.wfengine.InputNamePerformer"/>
      <branch>
        <sequence>
          <task name="Test1" class="org.pgist.wfengine.InputNamePerformer"/>
          <task name="Test2" class="org.pgist.wfengine.InputNamePerformer"/>
        </sequence>
        <sequence>
          <task name="Test3" class="org.pgist.wfengine.InputNamePerformer"/>
          <task name="Test4" class="org.pgist.wfengine.InputNamePerformer"/>
        </sequence>
        <sequence>
          <task name="Test5" class="org.pgist.wfengine.InputNamePerformer"/>
          <switch>
            <sequence>
              <task name="TestX001" class="org.pgist.wfengine.InputNamePerformer"/>
              <task name="TestX002" class="org.pgist.wfengine.InputNamePerformer"/>
              <task name="TestX003" class="org.pgist.wfengine.InputNamePerformer"/>
            </sequence>
            <sequence>
              <task name="TestX011" class="org.pgist.wfengine.InputNamePerformer"/>
              <task name="TestX012" class="org.pgist.wfengine.InputNamePerformer"/>
              <task name="TestX013" class="org.pgist.wfengine.InputNamePerformer"/>
            </sequence>
            <sequence>
              <task name="TestX021" class="org.pgist.wfengine.InputNamePerformer"/>
              <task name="TestX022" class="org.pgist.wfengine.InputNamePerformer"/>
              <task name="TestX023" class="org.pgist.wfengine.InputNamePerformer"/>
            </sequence>
          </switch>
          <task name="Test6" class="org.pgist.wfengine.InputNamePerformer"/>
          <task name="Test7" class="org.pgist.wfengine.InputNamePerformer"/>
          <task name="Test8" class="org.pgist.wfengine.InputNamePerformer"/>
        </sequence>
      </branch>
    </sequence>
  </process>
  
  
  <process name="process2">
    <environment>
      <var name="totalCount" value="1"/>
    </environment>
    <sequence>
      <task reference="process1">
      </task>
      <task name="Goodbye" class="org.pgist.wfengine.GoodbyePerformer">
      </task>
    </sequence>
  </process>
  
  
</workflow>

